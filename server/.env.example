DATABASE_URL="postgresql://postgres:password@localhost:5432/ai_use_case_catalog?schema=public"
PORT=3001
NODE_ENV=development

# Auth Configuration
# Allowed email domains (comma-separated)
AUTH_EMAIL_DOMAIN=ssaandco.com

# Admin emails (comma-separated) - these users get ADMIN role
ADMIN_EMAILS=admin@ssaandco.com

# Committee emails (comma-separated) - these users get COMMITTEE role
COMMITTEE_EMAILS=committee@ssaandco.com

# Development fallback admin email (used when no auth headers present in dev)
DEV_ADMIN_EMAIL=admin@ssaandco.com

# Impersonate a specific user in development (optional)
# DEV_IMPERSONATE_EMAIL=user@ssaandco.com
